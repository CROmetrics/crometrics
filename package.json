{
  "name": "clearhead",
  "version": "0.0.3",
  "description": "Clearhead Dev Module Library",
  "scripts": {
    "test": "gulp test",
    "compile": "babel --plugins object-assign -d ./lib/ src/",
    "prepublish": "npm run compile",
    "postinstall": "cp -r ./lib/ ./",
    "bump": "npm run test && mversion patch -m && npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clearhead/clearhead.git"
  },
  "bugs": {
    "url": "https://github.com/clearhead/clearhead/issues"
  },
  "homepage": "https://github.com/clearhead/clearhead#readme",
  "author": "casecode <casey.r.white@gmail.com>",
  "contributors": [
    {
      "name": "tomfuertes",
      "email": "tomfuertes@gmail.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-core": "^5.8.20",
    "babel-plugin-object-assign": "^1.2.1",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "chai": "^3.2.0",
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^0.15.0",
    "gulp-mocha": "^2.1.3",
    "gulp-mocha-phantomjs": "^0.8.1",
    "gulp-sequence": "^0.4.0",
    "jsdom": "^3.1.2",
    "mocha": "^2.2.5",
    "mocha-jsdom": "^0.3.0",
    "mocha-phantomjs": "^3.6.0",
    "mversion": "^1.10.0",
    "phantomjs": "^1.9.7-15",
    "sinon": "^1.15.4",
    "through2": "^2.0.0"
  }
}

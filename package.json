      "email": "eric@crometrics.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.0",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.2.0",

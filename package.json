    "karma-browserify": "^5.0.3",
    "karma-chai": "^0.1.0",
    "karma-es6-shim": "0.2.3",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "mocha": "^2.2.5",
    "mocha-jsdom": "^1.0.0",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.6",
    "rimraf": "^2.5.2",
